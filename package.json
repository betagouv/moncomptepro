{
  "name": "moncomptepro",
  "version": "1.0.0",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": "20.12.2"
  },
  "packageManager": "npm@10.5.1+sha256.06b9421a6b708ed6b6c0840e54729851e067b45ad911b192ebee2cb03b0adef2",
  "scripts": {
    "lint": "prettier --write */*.ts",
    "start": "tsx src/index.ts",
    "dev": "NODE_ENV=development concurrently \"npm:watch:*\"",
    "build": "run-s build:**",
    "build:assets": "vite build --clearScreen false",
    "build:db:migrate": "node-pg-migrate up",
    "build:db:conditionaly-load-fixtures": "if [ \"$RUN_FIXTURES\" = \"True\" ]; then npm run fixtures:load; fi",
    "update-organization-info": "tsx scripts/update-organizations-info.ts",
    "migrate": "node-pg-migrate",
    "fixtures:load": "node-pg-migrate up && tsx scripts/run-sql-script.ts scripts/fixtures.sql && npm run update-organization-info 2000",
    "fixtures:load-ci": "node-pg-migrate up && tsx scripts/run-sql-script.ts",
    "delete-database": "if [ \"$ENABLE_DATABASE_DELETION\" = \"True\" ]; then tsx scripts/run-sql-script.ts scripts/delete_all.sql; fi",
    "test": "run-s test:*",
    "test:lint": "prettier */*.ts --list-different",
    "test:unit": "CONSIDER_ALL_EMAIL_DOMAINS_AS_NON_FREE=False DO_NOT_USE_ANNUAIRE_EMAILS=False DO_NOT_CHECK_EMAIL_DELIVERABILITY=False mocha --require tsx test/*.ts",
    "watch:js": "tsc --watch --preserveWatchOutput",
    "watch:node": "tsx --watch src/index.ts",
    "watch:ejs": "CHOKIDAR_USEPOLLING=true copy-and-watch --clean --watch \"src/views/**/*.ejs\" src/views/*.ejs build/views/",
    "watch:assets": "NODE_ENV=development vite build --clearScreen false --watch"
  },
  "license": "AGPL-3.0",
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@gouvfr/dsfr": "~1.11.2",
    "@panva/jose": "^1.9.3",
    "@sentry/node": "^7.112.2",
    "@sentry/tracing": "^7.112.2",
    "@simplewebauthn/browser": "^8.3.4",
    "@simplewebauthn/server": "^8.3.5",
    "@simplewebauthn/typescript-types": "^8.3.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/console-log-level": "^1.4.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
